{
  "name": "apps-mobile",
  "version": "0.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
  "prestart": "pnpm -C ../../packages/ui run build",
  "dev:mobile": "pwsh -NoProfile -ExecutionPolicy Bypass -File ../../scripts/link-ui-node-modules.ps1 -ProjectRoot ..\\.. && pnpm -C ../../packages/ui run build && expo start --clear",
  "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint --ext .ts,.tsx src",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\""
  },
  "dependencies": {
    "expo": "~48.0.0",
    "react": "^18.2.0",
    "react-native": "0.71.14",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/material-top-tabs": "^6.2.1",
    "react-native-tab-view": "^3.5.1",
  "use-latest-callback": "0.2.4",
  "react-native-screens": "~3.20.0",
    "react-native-safe-area-context": "^4.5.0"
  },
  "devDependencies": {
    "nativewind": "^4.2.1",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.4.0"
  }
}
